public class Person {
  private String name;
  private Date dob;

  public Person(String name, Date dob){
    this.name = name;
    this.dob = dob;
  }
  public String getName(){
    return name;
  }
  public Date getDob(){
    return dob;
  }
  public void setName(String name){
    this.name = name;
  }
  public void setDob(Date dob){
    this.dob = dob;
  }
}
public class Person {
  private String name;
  private Date dob;

  public Person(String name, Date dob){
    this.name = name;
    this.dob = dob;
  }
  public String getName(){
    return name;
  }
  public Date getDob(){
    return dob;
  }
  public void setName(String name){
    this.name = name;
  }
  public void setDob(Date dob){
    this.dob = dob;
  }
}
